// Description :
//test of the verification if a number is a palindrome
//
// Results :
//ok
{
    int r,sum=0,temp;
        int n=454;//It is the number variable to be checked for palindrome

        temp=n;
        while(n>0){
            r=n%10;  //getting remainder
            sum=(sum*10)+r;
            n=n/10;
        }
        if(temp==sum){
            println("palindrome number ");
        }
        else{
            println("not palindrome");
        }
}
