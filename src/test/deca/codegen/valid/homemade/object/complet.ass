	LOAD -2(LB), R1
	LOAD #0, R0
	STORE R0, 1(R1)
; restore registers
	RTS
Code.Object.equals:
	LOAD -2(LB), R0
	LOAD -3(LB), R1
	CMP R0, R1
	SEQ R0
	RTS
code.A.getX:
	TSTO #1	; Max nb of stored register : 1 , ADDSP : 0
	BOV stackOverflow
; save registers
	PUSH R2
	LOAD -2(LB), R2
	LOAD 1(R2), R2
	LOAD R2, R0
	BRA fin.A.getX
	WSTR "ERROR : exited method A.getX with non-void return type without a return instruction."
	WNL
	ERROR
fin.A.getX:
; restore registers
	POP R2
	RTS
code.A.setX:
	TSTO #2	; Max nb of stored register : 2 , ADDSP : 0
	BOV stackOverflow
; save registers
	PUSH R3
	PUSH R2
	LOAD -3(LB), R2
	LOAD -2(LB), R3
	CMP #null, R3
	BEQ nullDereferencement
	STORE R2, 1(R3)
fin.A.setX:
; restore registers
	POP R2
	POP R3
	RTS
heapFull:
