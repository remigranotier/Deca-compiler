	ADDSP #1
	STORE R2, 0(SP)
	LOAD 0(SP), R2
	CMP #null, R2
	BEQ nullDereferencement
	LOAD 0(R2), R2
	BSR 2(R2)
	SUBSP #1
	LOAD R0, R1
	WINT
	WNL
	HALT
init.object:
	RTS
init.A:
